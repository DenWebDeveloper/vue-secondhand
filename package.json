{
  "name": "secondhand",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "axios": "^0.18.0",
    "js-cookie": "^2.2.0",
    "material-icons": "^0.2.3",
    "semantic-ui-css": "^2.4.0",
    "semantic-ui-vue": "^0.4.1",
    "vue": "^2.5.17",
    "vue-js-toggle-button": "^1.3.0",
    "vue-mdc": "0.0.10",
    "vue-router": "^3.0.1",
    "vuejs-paginate": "^2.0.1",
    "vuesax": "^3.4.5",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "@vue/eslint-config-prettier": "^3.0.4",
    "eslint-plugin-vue": "^4.7.1",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "rules": {
      "includeExports": true,
      "indent": [
        "error",
        "tab"
      ],
      "vue/script-indent": [
        "error",
        4,
        {
          "baseIndent": 1
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
